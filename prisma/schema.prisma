generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./transactions.db"
}


model Transactions {
  ID  BigInt @id @default(autoincrement())
  MerchantRequestID String @unique
  CheckoutRequestID String @unique
  ResultCode String
  ResultDesc String
  Amount String
  MpesaReceiptNumber String
  TransactionDate String
  PhoneNumber String
}
